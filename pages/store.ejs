    <!-- Initialize Swiper -->
    <script>
        var swiper = new Swiper(".mySwiper", {

            slidesPerView: 1,
            grid: {
                rows: 2,
            },
            spaceBetween: 10,
            pagination: {
                el: ".swiper-pagination",
                clickable: true,
            },
            // adjust the slides per view based on the size of screen
            breakpoints: {
            
               640: {
                    slidesPerView: 2,
                    grid: {
                        rows: 2,
                    },
                    spaceBetween: 10,
                },
                768: {
                    slidesPerView: 3,
                    grid: {
                        rows: 2,
                    },
                    spaceBetween: 10,
                },
                1024 : {
                    slidesPerView: 4,
                    grid: {
                        rows: 2,
                    },
                    spaceBetween: 10,
                },

            }
        });
    </script>
