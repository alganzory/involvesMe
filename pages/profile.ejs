  <!-- Post Swiper -->
  <div class="container ">
    <div class="row">
      <div class="col">
      <h1 class="profile-title text-left mb-6" contenteditable="true">Posts</h1>
    </div>

    <!-- Add New Button -->
    <div class="col ">
      <button class=" btn btn-simple btn-primary " style="float: right;">
        <i class="tim-icons icon-book-bookmark"></i> Add Now
      </button>
    </div>
      

    <% if(posts.length ===1){%>
    <div class="swiper mySingleSwiper ">
      <div class="swiper-wrapper justify-content-center">
    
        <div class="swiper-slide w-70 ">
          <!-- Post Card -->
          <% 
          var content;
          var s1 = null, s2 = null; 
          if(posts[0].content.length > 250){
            var s = posts[0].content.slice(0,100);
            var middle = Math.floor(s.length / 2);
            var before = s.lastIndexOf(' ', middle);
            var after = s.indexOf(' ', middle + 1);

            if (middle - before < after - middle) {
                middle = before;
            } else {
                middle = after;
            }
            s1 = s.substr(0, middle);
            s2 = s.substr(middle + 1);
          }  
        %>
          <div class="row mt-5" style="width: 80%; text-align: left;">
            <div class="col">
              <%if(s1 != null){content = s}else{content = posts[0].content}%>
                <blockquote class="blockquote blockquote-custom bg-white p-5 shadow rounded">
                    <div class="blockquote-custom-icon bg-primary  shadow-sm"><i class="fa fa-quote-left text-white"></i></div>
                    <p class="text-primary fw-normal"><%=posts[0].title%></p>
                    <footer class="pt-4  border-top">
                      <p class="card-title text-dark fw-normal" style="height: 135px;"><%=content%>
                        <%if(s1) {%>...<a href="/content/<%=posts[index]._id%>"><cite title="Source Title" class="p-1 text-info">read more</cite></a>
                      <%}%></p>
                      
                    </footer>
                </blockquote>

            </div>
        </div>
        </div>
        </div>
        </div>
        <%}%>
      <!-- Swiper -->
      <div class="swiper mySwiper">
        <div class="swiper-wrapper">
          <% if(posts.length != 1){ for (let index = 0; index < posts.length; index++) { %>
            
          <% if(posts.length < 2){%>
            <div class="">
          <% }else{ %> 
          <div class="swiper-slide">
            <% } %> 
            <!-- Post Card -->
            <% 
              var content;
              var s1 = null, s2 = null; 
              if(posts[index].content.length > 250){
                var s = posts[index].content.slice(0,100);
                var middle = Math.floor(s.length / 2);
                var before = s.lastIndexOf(' ', middle);
                var after = s.indexOf(' ', middle + 1);

                if (middle - before < after - middle) {
                    middle = before;
                } else {
                    middle = after;
                }
                s1 = s.substr(0, middle);
                s2 = s.substr(middle + 1);
              }  
            %>
              <div class="row mt-5" style="width: 80%; text-align: left;">
                <div class="col">
                  <%if(s1 != null){content = s}else{content = posts[index].content}%>
                    <blockquote class="blockquote blockquote-custom bg-white p-5 shadow rounded">
                        <div class="blockquote-custom-icon bg-primary  shadow-sm"><i class="fa fa-quote-left text-white"></i></div>
                        <p class="text-primary fw-normal"><%=posts[index].title%></p>
                        <footer class="pt-4  border-top">
                          <p class="card-title text-dark fw-normal" style="height: 135px;"><%=content%>
                            <%if(s1) {%>...<a href="/content/<%=posts[index]._id%>"><cite title="Source Title" class="p-1 text-info">read more</cite></a>
                          <%}%></p>
                          
                        </footer>
                    </blockquote>
                </div>
              </div>
          </div>
            <% }}%>
            </div>
            <div class="swiper-pagination"></div>
        </div>
      </div>
    </div>
  </div>
  
